<?xml version="1.0" encoding="utf-8"?>
<configuration>
    <system.serviceModel>
        <behaviors>
            <serviceBehaviors>
                <behavior name="NewBehavior0">
                    <serviceMetadata />
                </behavior>
            </serviceBehaviors>
        </behaviors>
        <bindings>
            <netTcpBinding>
                <binding name="BindingWCF">
                    <security mode="Message">
                        <message clientCredentialType="UserName" algorithmSuite="Basic128" />
                    </security>
                </binding>
            </netTcpBinding>
        </bindings>
        <services>
            <service behaviorConfiguration="NewBehavior0" name="Middleware.SVC.SVC">
                <endpoint address="net.tcp://localhost:2020/SERVERWCF" binding="netTcpBinding"
                    bindingConfiguration="BindingWCF" name="serviceWCF" contract="WCFContract.IServer" />
                <endpoint address="net.tcp://localhost:2029/SERVERWCF" binding="mexTcpBinding"
                    bindingConfiguration="" name="mexWCF" contract="IMetadataExchange" />
            </service>
        </services>
    </system.serviceModel>
</configuration>