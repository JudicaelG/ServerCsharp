<?xml version="1.0" encoding="utf-8"?>
<configuration>
    <system.serviceModel>
        <client />
        <behaviors>
            <serviceBehaviors>
                <behavior name="NewBehavior0">
                    <serviceMetadata/>
                </behavior>
            </serviceBehaviors>
        </behaviors>
        <bindings>
            <netTcpBinding>
                <binding name="BindingWCF">
                    <security mode="Message">
                        <transport clientCredentialType="None" protectionLevel="None" />
                        <message clientCredentialType="Windows" />
                    </security>
                </binding>
            </netTcpBinding>
        </bindings>
        <services>
            <service behaviorConfiguration="NewBehavior0" name="Middleware.SVC.SVC">
                <endpoint address="net.tcp://localhost:8010/SERVERWCF" binding="netTcpBinding" bindingConfiguration="BindingWCF" name="serviceWCF" contract="WCFContract.IServer"/>
                <endpoint address="net.tcp://localhost:8011/SERVERWCF" binding="mexTcpBinding" bindingConfiguration="" name="mexWCF" contract="IMetadataExchange"/>
            </service>
        </services>
    </system.serviceModel>
<startup><supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.6.1"/></startup></configuration>
